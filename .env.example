# API Configuration
EXPO_PUBLIC_API_URL=https://your-garnet-api.com
EXPO_PUBLIC_AI_ENGINE_URL=https://your-ai-engine.com

# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-app.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-app.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
EXPO_PUBLIC_FIREBASE_APP_ID=1:123456789:web:abcdef

# AWS S3 Configuration
EXPO_PUBLIC_AWS_REGION=us-east-1
EXPO_PUBLIC_AWS_BUCKET=garnet-media
EXPO_PUBLIC_AWS_ACCESS_KEY_ID=your_access_key
EXPO_PUBLIC_AWS_SECRET_ACCESS_KEY=your_secret_key

# Stripe Configuration
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_xxxxx

# YouTube API
EXPO_PUBLIC_YOUTUBE_CLIENT_ID=your_youtube_client_id
EXPO_PUBLIC_YOUTUBE_CLIENT_SECRET=your_youtube_client_secret

# Instagram API
EXPO_PUBLIC_INSTAGRAM_CLIENT_ID=your_instagram_client_id
EXPO_PUBLIC_INSTAGRAM_CLIENT_SECRET=your_instagram_client_secret

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_ENABLE_ANALYTICS=false
